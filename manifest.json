{
  "manifest_version": 2,
  "name": "Google Meet PATTATT",
  "version": "1.3",

  "browser_specific_settings": {
    "gecko": {
      "id": "pattatt@vedanth-p"
    }
  },

  "description": "A push-to-talk implementation for Google Meet.",

  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["pattatt.js"]
    }
  ]

}
